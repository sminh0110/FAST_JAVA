package ch14;

public class Subway {
	int subwayLine;
	int passengerCount;
	int bill= 1350;
	int money;
	
	public Subway(int subwayLine) {
		this.subwayLine = subwayLine;
	}
	
	public void take() {
		this.money += bill;
		passengerCount++;
	}
	
	public int getsubwayLine() {
		return subwayLine;
	}
	
	public void showBusInfo() {
		System.out.println(subwayLine+"번 버스 승객은 "+passengerCount+"명");
	}
}
