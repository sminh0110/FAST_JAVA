package ch09;

public class MainForScore {
	public static void main(String[] args) {
		Student st1 = new Student(0001, "신민호");
		Student st2 = new Student(0002, "황인겸");
		
		st1.setKoreaSubject("국어", 50);
		st1.setMathSubject("수학A", 50);
		
		st2.setKoreaSubject("국어", 40);
		st2.setMathSubject("수학B", 60);
	}
}
