package ch24;

import java.util.ArrayList;

public class ResultTest {
	public static void main(String[] args) {
		ArrayList<Student> students = new ArrayList<Student>();
		students.add(new Student("민호"));
		students.add(new Student("정수"));
		
		students.get(0).setStudying(new Subject("국어", 88));
		students.get(0).setStudying(new Subject("수학A", 80));
		
		students.get(1).setStudying(new Subject("국어", 90));
		students.get(1).setStudying(new Subject("수학B", 88));
		students.get(1).setStudying(new Subject("일본어", 100));
	}
}
